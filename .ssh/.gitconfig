[credential]
  helper = cache

[core]
  editor = vim
  pager = less -r
  autocrlf = false

[user]
	name = Yordan Georgiev
	email = %email%

[push]
   default = simple
   followTags = true

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true

[color "status"]
  added = green
  changed = red bold
  untracked = magenta bold

[color "branch"]
  remote = yellow

[fetch]
   prune = true
